score = input("Enter Score: ")
sc = float(score)
if 0.0 <= sc <= 1.0:
    if sc >= 0.9:
        result = "A"
        print (result)
    elif 0.8 <= sc < 0.9:
        result = "B"
        print (result)
    elif 0.7 <= sc < 0.8:
        result = "C"
        print (result)
    elif 0.6 <= sc < 0.7:
        result = "D"
        print (result)
    else:
        result = "F"
        print (result)
else:
    print ("Error: score out of range")
